public Boolean isPassed(Boolean passedExam, Boolean passedAssignments, Boolean passedProject) {
    boolean passed; //VARIABLE TO BE RETURNED

    //CHECK FOR THE CONDITIONS, RETURN TRUE
    if(passedExam == true && passedAssignments == true) {
        passed = true;
    }
    else if(passedProject == true && passedAssignments == true) {
        passed = true;
    }
    else if(passedExam == true && passedProject == true) {
        passed = true;
    }

    //IF NO CONDITION IS MATCHED, RETURN FALSE
    else passed = false;

    return passed;
}